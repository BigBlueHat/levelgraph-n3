{
  "name": "levelgraph-n3",
  "version": "1.0.0",
  "description": "LevelGraph plugin that add the ability to load and store n3/turtle files.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec test",
    "zuul": "zuul test/common.js test/*spec.js",
    "zuul-local": "zuul --open --local 8080 -- test/common.js test/*spec.js",
    "coverage": "rm -rf coverage; istanbul cover _mocha -- --recursive --reporter spec --bail",
    "publish-coverage": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcollina/levelgraph-n3.git"
  },
  "bugs": {
    "url": "http://github.com/mcollina/levelgraph-n3/issues"
  },
  "keywords": [
    "levelgraph",
    "graph",
    "rdf",
    "turtle",
    "linked",
    "data",
    "linked data",
    "n3"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "dependencies": {
    "cli": "^0.6.5",
    "concat-stream": "~1.4.6",
    "end-of-stream": "^1.1.0",
    "glob": "^5.0.3",
    "level": "^1.1.0",
    "mkdirp": "^0.5.0",
    "n3": "^0.4.3",
    "progress-stream": "^1.0.1",
    "readable-stream": ">= 1.1.13 < 2.0.0"
  },
  "peerDependencies": {
    "levelgraph": ">= 1.0 < 2.0"
  },
  "devDependencies": {
    "mocha": "~2.2.1",
    "levelgraph": "^1.0.0",
    "level-test": "^2.0.0",
    "chai": "~1.10.0",
    "coveralls": "~2.11.2",
    "istanbul": "~0.3.2",
    "zuul": "~3.0.0",
    "browserify": "~10.2.4",
    "uglify-js": "~2.4.15"
  }
}
